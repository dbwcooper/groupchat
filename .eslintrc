{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "generator-star-spacing": [0],
    "semi": ["off"],
    "consistent-return": [0],
    "react/forbid-prop-types": [0],
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "global-require": [1],
    "import/prefer-default-export": [0],
    "react/jsx-no-bind": [0],
    "react/prop-types": [0],
    "react/prefer-stateless-function": [0],
    "react/jsx-boolean-value": [0],
    "no-else-return": [0],
    "no-restricted-syntax": [0],
    "import/no-extraneous-dependencies": [0],
    "no-use-before-define": [0],
    "jsx-a11y/no-static-element-interactions": [0],
    "no-nested-ternary": [0],
    "arrow-body-style": [0],
    "import/extensions": [0],
    "no-bitwise": [0],
    "no-mixed-operators": [0],
    "no-cond-assign": [0],
    "import/no-unresolved": [0],
    "require-yield": [1],
    "linebreak-style": [0],
    "quote-props": [0],
    "prefer-template": [0],
    "no-param-reassign": [0],
    "no-plusplus": [0],
    "comma-dangle": [0],
    "object-shorthand": [0],
    "no-extend-native": [0],
    "dot-notation": [0],
    "prefer-const": [0],
    "no-console": ["off"],
    "no-debugger": ["off"],
    "react/no-danger": [0],
    "func-names": [0]
  },
  "globals": {
    "document": true, 
    "window": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
